{
  "schema": {
      "name": "boss-v0.1-schema",
      "validator": "BossValidatorV01"
  },
  "client": {
    "backend": {
      "name": "boss",
      "class": "BossBackend",
      "host": "api.theboss.io",
      "protocol": "https"
    },
    "path_processor": {
      "class": "ingest.plugins.hdf5.Hdf5TimeSeriesPathProcessor",
      "params": {
        "root_dir": "",
        "extension": "hdf5",
        "base_filename": "test_id_slice<o:1>"
      }
    },
    "tile_processor": {
      "class": "ingest.plugins.hdf5.Hdf5TimeSeriesTileProcessor",
      "params": {
        "filesystem": "local",
        "bucket": "",
        "upload_format": "tiff",
        "channel_index": 0,
        "scale_factor": 0.0001,
        "dataset": "aligned"
      }
    }
  },
  "database": {
    "collection": "my_col_1",
    "experiment": "my_exp_1",
    "channel": "my_ch_1"
  },
  "ingest_job": {
    "resolution": 0,
    "extent": {
      "x": [0, 768],
      "y": [0, 512],
      "z": [0, 3],
      "t": [0, 30]
    },
    "tile_size": {
      "x": 768,
      "y": 512,
      "z": 1,
      "t": 1
    }
  }
}


